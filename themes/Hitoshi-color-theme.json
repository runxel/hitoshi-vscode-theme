				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#fac863"
			}
		},
		{
			"name": "Flow Control",
			"scope": [
				"keyword.control" // .conditional, .flow
			],
			"settings": {
				"foreground": "#8c6cec"
				//st:: "background": "color(var(back) blend(#fff 95%))"
			}
		},
		{
			"name": "Library Class",
			"scope": [
				"keyword.control" // .conditional, .flow
			],
			"settings": {
				"foreground": "#8c6cec"
			}
		},

{
	/* HITOSHI â€“ a dark Visual Studio Code theme with retro charme
	   https://github.com/runxel/hitoshi-vscode-theme */
	"$schema": "vscode://schemas/color-theme",
	"name": "Hitoshi",
	"author": "Lucas Becker / runxel",
	"type": "dark",
	/* INFO: https://code.visualstudio.com/api/references/theme-color */

	/* color variables (hopefully included @ some future point in time)
		main/back:	#3a3833
		main/dark:	#24211c //not for use in tokens
		main/fore:	#dbdbdb //offwhite
		main/grey:	#686a5b //used for comments
		lightgrey:	#93a1a1
		bluegrey:	#657b83
		pinch:		#586e75 //grey-blue
		yellow:		#fac863 //also accent
		orange:		#f99157
		red:		#ec5f67
		darkred:	#be534e
		magenta:	#d33682
		violet:		#8c6cec
		velvet:		#bcbcff
		blue:		#268bd2
		lightblue: 	#98d4f5
		petrol:		#2aa198
		cyan:		#3fedd8
		green:		#40bf80
		defgreen:	#7ae014
		slime:		#cad57d
		sand:		#dbca95
		brown:		#ab7967
	*/
	"colors": {
		/*** UI COLORS ***/

		// basic
		"editor.background": "#3a3833", // main background color
		"editor.foreground": "#dbdbdb", // main text color
		"icon.foreground": "#dbdbdb",
		"editorCursor.foreground": "#40bf80", // caret color
		"selection.background": "#ff99007e", // selection color for workbench text
		"progressBar.background": "#fac863",

		"focusBorder": "#fac863",
		"widget.shadow": "#251f1570",

		// basic text colors
		"descriptionForeground": "#93a1a1b6",
		"textPreformat.foreground": "#98d4f5", // e.g. inline code in .md preview
		//"textSeparator.foreground": "#000", // doesn't work?
		"textBlockQuote.background": "#ffffff11",
		"textBlockQuote.border": "#fac86371",
		// "textCodeBlock.background": "#000",  // doesn't work?
		"textLink.foreground": "#fac863",
		"textLink.activeForeground": "#a58809",

		// welcome page
		"welcomePage.buttonBackground": "#24211c",
		"welcomePage.buttonHoverBackground": "#fac8635d",
		
		// window titlebar
		"titleBar.activeBackground": "#24211c",
		"titleBar.activeForeground": "#dbdbdba8",
		"titleBar.inactiveBackground": "#24211c",
		"titleBar.inactiveForeground": "#dbdbdb3d",

		// current line styles (where caret is)
		"editor.lineHighlightBackground": "#ffffff27",
		"editor.lineHighlightBorder": null,  // border here looks stupid
		"editor.rangeHighlightBackground": "#a7880080",

		// text selection colors
		"editor.selectionBackground": "#ff99007e",
		// 'same content as selection' highlighting
		"editor.selectionHighlightBackground": "#302c2196",
		"editor.selectionHighlightBorder": "#ffbb00",
		// selection color when inactive
		"editor.inactiveSelectionBackground": "#302c2196",
		//??? "editor.wordHighlightStrongBackground": "#000",
		//??? "editor.wordHighlightBackground": "#000",

		// activityBar ("Sidebar")
		"activityBar.background": "#35332e",
		"activityBar.border": "#2f2d2950",
		"activityBar.dropBackground": "#2a2824a6",
		"activityBar.foreground": "#dbdbdbcc",
		"activityBarBadge.background": "#ffbb00b9",
		"activityBarBadge.foreground": "#3a3833",

		// badge
		"badge.background": "#ffbb00",
		"badge.foreground": "#3a3833",

		// button
		"button.background": "#ffbb00",
		"button.foreground": "#3a3833",
		"button.hoverBackground": "#ec9200",
		
		// gutter
		"editorGutter.background": "#3a3833",
		"editorGutter.addedBackground": "#3C9F4A",
		"editorGutter.deletedBackground": "#A22929",
		"editorGutter.modifiedBackground": "#26506D",

		// breadcrumb
		"breadcrumb.foreground": "#dbdbdb75",
		"breadcrumb.background": "#35332e",
		"breadcrumb.focusForeground": "#dbdbdb",
		"breadcrumb.activeSelectionForeground": "#ffffffee",
		"breadcrumbPicker.background": "#35332e",

		// editor
		"editorHoverWidget.foreground": "#000", // ???? doesn't work
		"editorHoverWidget.background": "#24211c", // e.g. scope panel
		"editorHoverWidget.border": "#424138",
		"editorIndentGuide.background": "#444444", // when unactive (cursor outside)
		"editorIndentGuide.activeBackground": "#fac86362", // when clicked inside indent
		"editorLineNumber.foreground": "#858585",
		"editorLineNumber.activeForeground": "#b4b4b4",
		"editorLink.activeForeground": "#858585",
		"editorBracketMatch.border": "#cad57d",		// matching brackets
		"editorBracketMatch.background": "#24211c", //   - " -
		"editorRuler.foreground": "#686a5b8a", // ruler color

		// search
		"editor.findMatchBackground": "#f99157",

		// statusBar
		"statusBar.background": "#24211c",
		"statusBar.border": "#313125",
		"statusBar.debuggingBackground": "#2d2515",
		"statusBar.debuggingBorder": "#ffbb00",
		"statusBar.debuggingForeground": "#fac863",
		"statusBar.foreground": "#aaa",
		"statusBar.noFolderBackground": "#24211c",
		"statusBar.noFolderBorder": "#580d45",
		"statusBar.noFolderForeground": "#aaa",
		"statusBarItem.activeBackground": "#997631",
		"statusBarItem.hoverBackground": "#fac8635d",
		"statusBarItem.prominentBackground": "#15232d",
		"statusBarItem.prominentHoverBackground": "#0d3a58",

		// tab
		"tab.activeBackground": "#3a3833",
		"tab.activeForeground": "#dbdbdb",
		"tab.border": "#201e17",
		"tab.activeBorderTop": "#fac863",
		"tab.inactiveBackground": "#24211c",
		"tab.inactiveForeground": "#dbdbdb3d",
		"tab.unfocusedActiveForeground": "#dbdbdb",
		"tab.unfocusedInactiveForeground": "#dbdbdb3d",
		"tab.unfocusedActiveBorderTop": "#3a3833",
		"tab.hoverBackground": "#fac8633d",

		// input
		"input.background": "#2d2c27",
		"input.border": "#fac8633d",
		"input.foreground": "#dbdbdb",
		"input.placeholderForeground": "#dbdbdb9d",
		"inputOption.activeBorder": "#1e241c",
		"inputOption.activeBackground": "#b37c10",  // BG of active items
		"inputValidation.errorBackground": "#193549",
		"inputValidation.errorBorder": "#ffc600",
		"inputValidation.infoBackground": "#491f19",
		"inputValidation.infoBorder": "#b92f2f",
		"inputValidation.warningBackground": "#431949",
		"inputValidation.warningBorder": "#ffc600",

		// dropdown
		"dropdown.foreground": "#dbdbdb",
		"dropdown.background": "#24211c",
		"dropdown.listBackground": "#24211c",
		"dropdown.border": "#646463",

		// quick input panel
		"quickInput.foreground": "#dbdbdb",
		"quickInput.background": "#24211c",
		"pickerGroup.foreground": "#fac863",
		"pickerGroup.border": "#494947",

		// minimap
		"minimap.findMatchHighlight": "#fac863",
		"minimap.selectionHighlight": "#fac863",
		"minimap.errorHighlight": "#b62f2f",
		"minimap.warningHighlight": "#e99f16",
		"minimapGutter.addedBackground": "#cae07a",
		"minimapGutter.modifiedBackground": "#d6c278",
		"minimapGutter.deletedBackground": "#f0d0d9",

		// tree/list
		"tree.indentGuidesStroke": "#50504f",
		"list.activeSelectionBackground": "#b68c39",
		"list.focusForeground": "#24211c", // also used in quick picker
		"list.focusBackground": "#b68c39", // dito
		"list.hoverForeground": "#181613",
		"list.hoverBackground": "#93a1a1",

		// Git status colors in File Explorer
		"gitDecoration.modifiedResourceForeground": "#d6c278",    //modified
		"gitDecoration.deletedResourceForeground": "#f0d0d9",     //deleted
		"gitDecoration.untrackedResourceForeground": "#cae07a",   //untracked
		"gitDecoration.ignoredResourceForeground": "#808080",     //ignored
		"gitDecoration.conflictingResourceForeground": "#ec7614", //conflict

		// merge conflicts  / TODO
		// "merge.currentHeaderBackground": "#ff0000",
		// "merge.currentContentBackground": "#ff0000",
		// "merge.incomingHeaderBackground": "#ff0000",
		// "merge.incomingContentBackground": "#ff0000",
		// "merge.border": "#ff0000",
		// "editorOverviewRuler.currentContentForeground": "#ff0000",
		// "editorOverviewRuler.incomingContentForeground": "#ff0000",

		// settings
		"settings.modifiedItemIndicator": "#657b83",
		"settings.dropdownBackground": "#2d2c27",   // same as input.background
		"settings.checkboxBackground": "#2d2c27",   // dito
		"settings.checkboxBorder": "#fac8633d",     // same as input.border
		"settings.dropdownBorder": "#fac8633d",     // dito
		"settings.dropdownListBorder": "#fac8633d", // dito

		// extension ("install" button)
		"extensionButton.prominentForeground": "#24211c", 
		"extensionButton.prominentBackground": "#40bf80",
		"extensionButton.prominentHoverBackground": "#2f8d5e",
		
		// Terminal Colors
		"terminal.foreground": "#dbdbdb",  //text input color
		"terminal.background": "#35332e",  //background
		"terminal.ansiBlack": "#20201e",
		"terminal.ansiRed": "#ec5f67",
		"terminal.ansiGreen": "#40bf80",
		"terminal.ansiYellow": "#fac863",
		"terminal.ansiBlue": "#268bd2",
		"terminal.ansiMagenta": "#d33682",
		"terminal.ansiCyan": "#3fedd8",
		"terminal.ansiWhite": "#dbdbdb",
		"terminal.ansiBrightBlack": "#0050A4",
		"terminal.ansiBrightRed": "#ff628c",
		"terminal.ansiBrightGreen": "#3ad900",
		"terminal.ansiBrightYellow": "#ffc600",
		"terminal.ansiBrightBlue": "#0088ff",
		"terminal.ansiBrightMagenta": "#fb94ff",
		"terminal.ansiBrightCyan": "#80fcff",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminalCursor.background": "#fac863",  //caret
		"terminalCursor.foreground": "#fac863",



		"sideBarTitle.foreground": "#bbbbbb",

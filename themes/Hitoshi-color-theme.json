				"keyword.operator.comparison",
				"keyword.operator.assignment",
				"keyword.operator.arithmetic",
				"keyword.operator.logcial",
				"punctuation.definition.equals.ini",
				"keyword.operator.asp"
			],
			"settings": {
				"foreground": "#3fedd8"
			}
		},
		{
			"name": "Class Misc",
			"scope": [
				"keyword.control.class",
				"entity.name",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#fac863"
			}
		},
		{
			"name": "Function Name",
			"scope": [
				"entity.name.function",
				"entity.name.tag.structure.any",
				"entity.name.tag.block"
			],
			"settings": {
				"foreground": "#2aa198"
			}
		},
		{
			"name": "Inherited Class",

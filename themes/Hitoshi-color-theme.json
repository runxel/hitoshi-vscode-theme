	"tokenColors": [
		/*** SYNTAX COLORS ***/
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"meta.documentation"
			],
			"settings": {
				"foreground": "#686a5b"
			}
		},
		{
			// TODO, FIXME etc
			"name": "Highlight Words",
			"scope": [
				"keyword.codetag"
			],
			"settings": {
				"foreground": "#AB7967",
				//st:: "background": "#686a5b",
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				// "foreground": "#2aa198"
				"foreground": "#dbca95"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#3fedd8"
			}
		},
		{
			"name": "Escape Characters \\, Numbers, Function Arguments",
			"scope": [
				"constant.character.escape",
				"constant.numeric",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "Variables Function",
			"scope": [
				"variable.function"
			],
			"settings": {
				"foreground": "#98d4f5"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d33682"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#40bf80"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#93a1a1"
			}
		},
		{
			"name": "Storage Type",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#bcbcff"
			}
		},
		{
			"name": "Arithmetical, Assignment, Comparision Operators",

				"foreground": "#40bf80"
			}
		},
		{
			"name": "Variable start",
			"scope": [
				"punctuation.definition.variable"
				// not sure if this is even a thing in VSC
			],
			"settings": {
				"foreground": "#40bf80"
			}
		},
		{
			"name": "Built-in + User-defined constants",
			"scope": [
				"constant",
				"constant.language",
				"constant.character",
				"constant.other",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#dbdbdb"
			}
		},
		{
			"name": "Support constructs",
			"scope": [
				// again not sure if this is a thing :D
				"support.funtion.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#ec5f67"
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entitiy.name.tag"
			],
			"settings": {
				"foreground": "#dbca95"
			}
		},
		{
			// brackets & slashes in HTML tags: <foo></foo>
			"name": "Tag start/end",
			"scope": [
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#586e75"
			}
		},
		{
			"name": "Continuation",
			"scope": [

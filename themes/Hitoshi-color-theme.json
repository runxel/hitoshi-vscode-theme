			],
			"settings": {
				"foreground": "#ec5f67"
			}
		},
		{
			"name": "Exception",
			"scope": [
				"support.type.exception"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Special Method",
			"scope": [
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#f99157"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#f3405e"
				//st:: "foreground": "#d7e7db"
				//st:: "background": "#f86161"
			}
		},
		{
			"name": "Deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#aa7ca4"
				//st:: "foreground": "#d7e7db"
				//st:: "background": "#cc9ad8"
			}
		},
		{
			"name": "Square Brackets []",
			"scope": [
				"meta.brace.square",
				"punctuation.section.brackets"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "Round + Curly Brackets (),{}",
			"scope": [
				"meta.brace.round",
				"meta.brace.curly",
				"punctuation.section",
				"punctuation.section.block",
				"punctuation.definition.parameters",
				"punctuation.section.group",
				"punctuation.separator.dictionary",
				"punctuation.separator.comma.js",
				"punctuation.separator.arguments.python",
				"punctuation.separator.parameters.python"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			// not present in ST Hitoshi
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		
